{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "components/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "eslint . --fix"
    ],
    "pages/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "eslint . --fix"
    ]
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@apollo/react-ssr": "^3.1.5",
    "@material-ui/core": "^4.10.0",
    "@material-ui/icons": "^4.9.1",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "babel-core": "^6.26.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.10.7",
    "draft-js": "^0.11.5",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "lodash": "^4.17.15",
    "next": "^11.1.0",
    "next-apollo": "^4.0.1",
    "next-with-apollo": "^5.0.1",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-adopt": "^0.6.0",
    "react-apollo": "^3.1.5",
    "react-dom": "16.13.1",
    "react-simple-code-editor": "^0.11.0",
    "remark-parse": "^8.0.2",
    "remark-react": "^7.0.1",
    "styled-components": "^5.1.0",
    "unified": "^9.0.0"
  },
  "devDependencies": {
    "apollo": "^2.27.4",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "graphql-tools": "^5.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.7",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      }
    }
  }
}
